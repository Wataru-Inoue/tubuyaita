<h3>以下の内容で投稿します</h3>

<%= form_for(@tweet) do |f| %>
  <%= f.hidden_field :content %>
  <p>つぶやき：<%= @tweet.content %></p>
  <%= f.submit "投稿する" %>

<% end %>

<%= form_for(@tweet, url: new_tweet_path, method: 'get' ) do |f| %>
  <%= f.hidden_field :content %>
  <%= f.submit "戻る", name: 'back' %>
<% end %>